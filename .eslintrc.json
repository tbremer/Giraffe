{
  "plugins": [ "import" ],
  "extends": [ "tbremer", "plugin:import/errors", "plugin:import/warnings" ],
  "rules": {
    "max-params": [2, 4],
    "object-property-newline": [ 2, { "allowMultiplePropertiesPerLine": true } ],
    "newline-after-var": 0,
    "max-depth": [2, 7],
    "brace-style": [2, "stroustrup" ]
  }
}
